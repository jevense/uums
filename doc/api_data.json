[  {    "type": "post",    "url": "/login",    "title": "登录",    "name": "validate",    "version": "0.0.1",    "group": "login",    "examples": [      {        "title": "接口示例:",        "content": "curl -i https://api.mvwchina.com/basic-auth/login",        "type": "curl"      }    ],    "header": {      "fields": {        "Header": [          {            "group": "Header",            "type": "String",            "optional": false,            "field": "X-MVW-ENV",            "description": "<p>请求头必须携带字段X-MVW-ENV</p>"          }        ]      },      "examples": [        {          "title": "头部示例:",          "content": "{\n\"X-MVW-ENV\": {\n\"appID\": \"应用ID，应用在公司内部的编号\",\n\"appVersion\": \"应用版本\",\n\"deviceType\": \"设备类型，WEB/Android/IOS\",\n\"deviceModel\": \"设备型号\",\n\"deviceID\": \"设备ID\",\n\"pushToken\": \"发送PUSH的Token，对移动设备有意义\",\n\"ip\": \"IP地址，用来做一些异常检查和控制\",\n\"userAgent\": \"浏览器User-Agent\",\n\"osType\": \"设备操作系统类型\",\n\"osVersion\": \"设备操作系统版本\",\n\"market\": \"市场推广渠道\"\n}\n}",          "type": "json"        }      ]    },    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "allowedValues": [              "\"ACCOUNT_AND_PASSWORD\"",              "\"PHONE_AND_PASSWORD\"",              "\"PHONE_AND_CODE\""            ],            "optional": false,            "field": "type",            "description": "<p>验证类型： <br>ACCOUNT_AND_PASSWORD——帐号密码，<br>PHONE_AND_PASSWORD——手机号和密码，<br>PHONE_AND_CODE——手机号和验证码<br></p>"          },          {            "group": "Parameter",            "type": "String",            "size": "..18",            "optional": false,            "field": "account",            "description": "<p>登录帐号/手机号</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "password",            "description": "<p>密码/手机验证码</p>"          }        ]      },      "examples": [        {          "title": "请求示例:",          "content": "{\n\"type\": \"ACCOUNT_AND_PASSWORD\",\n\"account\": \"1234568999\",\n\"password\": \"*(@#@!!)!)\"\n}",          "type": "json"        }      ]    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Boolean",            "optional": false,            "field": "status",            "description": "<p>登录是否成功</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "userID",            "description": "<p>用户ID</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "token",            "description": "<p>用户令牌</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "errorMessage",            "description": "<p>登录失败原因</p>"          }        ]      },      "examples": [        {          "title": "成功响应示例:",          "content": "HTTP/1.1 200 OK\n{\n\"status\": true,\n\"userID\": \"98231423423453efa23es9232\"，\n\"token\": \"BWKSPPP+WSLQOWMRNBSIIWW\"\n}",          "type": "json"        },        {          "title": "失败响应示例:",          "content": "HTTP/1.1 200 OK\n{\n\"status\": false,\n\"errorMessage\": \"用户名密码不匹配\"\n}",          "type": "json"        }      ]    },    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "optional": false,            "field": "404",            "description": "<p>帐号不存在</p>"          }        ],        "Error 5xx": [          {            "group": "Error 5xx",            "optional": false,            "field": "503",            "description": "<p>服务不可用</p>"          }        ]      }    },    "filename": "basic-auth-webflux/src/main/java/com/mvwchina/funcation/basicauth/controller/LoginController.java",    "groupTitle": "login"  },  {    "type": "get",    "url": "/validate",    "title": "验证令牌",    "name": "validate",    "version": "0.0.1",    "group": "validate",    "examples": [      {        "title": "接口示例:",        "content": "curl -i https://api.mvwchina.com/basic-auth/validate",        "type": "curl"      }    ],    "header": {      "fields": {        "Header": [          {            "group": "Header",            "type": "String",            "optional": false,            "field": "X-MVW-userID",            "description": "<p>请求头必须携带字段X-MVW-userID</p>"          },          {            "group": "Header",            "type": "String",            "optional": false,            "field": "access-key",            "description": "<p>请求头必须携带字段access-key</p>"          }        ]      },      "examples": [        {          "title": "头部示例:",          "content": "{\n\"X-MVW-userID\": \"用户ID\",\n\"access-key\": \"按照约定加密方式产生的token==\"\n}",          "type": "json"        }      ]    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Boolean",            "optional": false,            "field": "status",            "description": "<p>验证是否成功</p>"          }        ]      },      "examples": [        {          "title": "响应示例:",          "content": "HTTP/1.1 200 OK\n{\n\"status\": true\n}",          "type": "json"        }      ]    },    "filename": "basic-auth-webflux/src/main/java/com/mvwchina/funcation/basicauth/controller/ValidateController.java",    "groupTitle": "validate"  }]
